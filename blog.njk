---
layout: layouts/article-layout.njk
pagination:
  data: collections.article
  size: 10
  reverse: true
  alias: articles
---
{% for article in articles %}
  <article>
    <h1>
      <a href="{{ article.url | url }}">{{ article.data.title }}</a>
    </h1>
    {{ article.date }}
  </article>
{% endfor %}